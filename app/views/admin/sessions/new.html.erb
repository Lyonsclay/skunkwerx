<h1>Log in</h1>
<div class="sessions">
	<%= form_for(:session, url: admin_sessions_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
	  <%= f.submit "Log in" %>
	  <p><%= link_to "forgotten password?", new_admin_password_reset_path %>
	<% end %>
</div>
